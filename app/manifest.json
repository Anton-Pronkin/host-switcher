{
    "name": "Host switcher",
    "version": "0.2.1",
    "description": "Let's switch host!",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
          "16": "images/icon16.png",
          "24": "images/icon24.png",
          "32": "images/icon32.png"
        },
        "default_title": "Switch host",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"]
}